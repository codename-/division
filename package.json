{
  "name": "division",
  "version": "1.0.0",
  "description": "Simple yet powerful wrapper over node.js cluster API. This module is inspired by impressive, but abandoned project Cluster created by TJ Holowaychuk.",
  "author": "codename-",
  "main": "lib/division.js",
  "devDependencies": {
    "coffee-script": "^1.8.0",
    "coffeelint": "^1.6.1",
    "coveralls": "^2.11.2",
